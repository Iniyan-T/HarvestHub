# Server Configuration
PORT=5000

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/harvesthub

# Gemini API Keys
GEMINI_API_KEY=your_gemini_vision_api_key_here
GEMINI_CHAT_API_KEY=your_gemini_chat_api_key_here

# Ollama Configuration (Local LLM)
USE_OLLAMA=true
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2

# OpenAI Configuration (Optional - if using OpenAI instead of Gemini)
OPENAI_API_KEY=your_openai_api_key_here
